{
  "name": "react-notes",
  "version": "0.1.0",
  "homepage": "https://DANIDIA.github.io/notes",
  "private": true,
  "scripts": {
    "predeploy": "env-cmd -f .env.deploy npm run build",
    "deploy": "gh-pages -d build",
    "start": "env-cmd -f .env.dev react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "stop": "taskkill -F -IM node.exe"
  },
  "dependencies": {
    "classnames": "^2.3.2",
    "env-cmd": "^10.1.0",
    "react": "^18.2.0",
    "react-confirm": "^0.3.0-7",
    "react-dom": "^18.2.0",
    "react-icons": "^4.11.0",
    "react-router-dom": "^6.16.0",
    "react-scripts": "5.0.1",
    "uuid": "^9.0.1",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^8.54.0",
    "gh-pages": "^6.0.0",
    "prettier": "^3.0.3"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "tabWidth": 4,
    "useTabs": false,
    "semi": true,
    "singleQuote": true
  }
}
